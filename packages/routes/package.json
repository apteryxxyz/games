{
    "name": "@owenii/routes",
    "version": "0.0.0",
    "exports": {
        "./api": {
            "require": "./api.js",
            "types": "./api.d.ts"
        }
    },
    "scripts": {
        "lint": "eslint --ext .js,.ts",
        "format": "prettier --write \"**/*.{js,ts}\" && npm run lint -- --fix",
        "build": "tsc"
    },
    "dependencies": {
        "@owenii/types": "workspace:^0.0.0",
        "@owenii/validators": "workspace:^0.0.0",
        "mongodb-memory-server": "^8.10.1",
        "mongoose": "^6.7.5"
    },
    "devDependencies": {
        "tsup": "^6.5.0",
        "typescript": "^4.9.3"
    },
    "nx": {
        "targets": {
            "format": {
                "dependsOn": ["^format"],
                "outputs": ["{projectRoot}/*"]
            },
            "build": {
                "dependsOn": ["^build"],
                "outputs": ["{projectRoot}/*"]
            }
        }
    }
}
